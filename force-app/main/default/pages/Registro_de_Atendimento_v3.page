                                <td class="titulo_3" colspan="2">
                                    DESCRIÇÕES
                                </td>
                            </tr>
                            <tr><td class="label">Descrição:</td>                   <td class="valor">{!WorkOrder.Description}</td></tr>
                            <apex:outputText rendered="{!IF(WorkOrder.Localiza_ao_Equipamento__c != '',true,false)}">
                            <tr><td class="label">Loc. Equip.:</td>                   <td class="valor">{!WorkOrder.Localiza_ao_Equipamento__c}</td></tr>
                            </apex:outputText>
