            </table>
        <br/>
        
        
            <!-- DADOS DO CLIENTE -->
            <table class="#" cellpadding="0px" cellspacing="0px" style="width:100%;">
                <tr style="margin:0; padding:0;">
                    <td style="width:*; margin:0; padding:0;">
                    <div  style="padding:0; margin: 0 0 -35px 0;" > 
                            <apex:image style="padding:0; margin:0 auto;" value="{!if(WorkOrder.tecnico_executor__r.assinatura__c != "", WorkOrder.tecnico_executor__r.assinatura__c, "https://hospcom--c.na170.content.force.com/servlet/servlet.ImageServer?id=0156e00000Bf8Ui&oid=00Di0000000JVhH&lastMod=1667927109000")}" width="200" height="50px"/>
                    </div>     

