        padding:2px 5px;
        line-height:120%;
        }.listagem .pre-cabecalho{
        height:10px;
        }.listagem .cabecalho td{
        font-size:12px;
        font-weight:bold;
        background-color:#e2e3e3;
        white-space: nowrap;
        border-color: black;
        border-style:solid;
        border-width:1px 0px 1px 0px;
        }.listagem .item-dados .imagem{
        vertical-align:middle;
        }.listagem .item-dados.acessorio .imagem{
        text-align:right;
