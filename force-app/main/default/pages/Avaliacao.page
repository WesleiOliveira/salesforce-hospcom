<apex:page showChat="False" title="Cadastro de Avaliação" showHeader="true" sidebar="false" docType="HTML-5.0" showQuickActionVfHeader="False" controller="AvaliacaoController">
    <html>
    <head>
        
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous"/>
        <style>
            body{margin:0px!important;border:0px!important;}
            #contentWrapper > div.bodyDiv.brdPalette.brandPrimaryBrd{margin:0px!important;border:0px!important;}
            #contentWrapper > div.bPageFooter.noTableFooter{display:none!important;}
            #contentWrapper > div.bPageFooter.noTableFooter > div{display:none!important;}
            #contentWrapper > div.bPageFooter.noTableFooter > div{display:none!important;}
            #AppBodyHeader{display:none!important;}
            #globalHeaderBar{display:none!important;}
            .sfdcBody{
                background: transparent!important;
            }
            #contentWrapper > div.bodyDiv.brdPalette.brandPrimaryBrd{
                box-shadow:none!important;
                background-color: transparent!important;
            }
            .form-control {
                background: transparent!important;
                color: #163757!important;
                border: 2px solid #163757!important;
            }
            .botao{
                color: #163757!important;
                background-color: #86D0F3!important;
                border-color: #86D0F3!important;
                font-size: 16px!important;
                display: block!important;
                width: 100%!important;
                display: inline-block!important;
                margin-bottom: 0!important;
                font-weight: normal!important;
                text-align: center;
                vertical-align: middle!important;
                -ms-touch-action: manipulation!important;
                touch-action: manipulation!important;
                cursor: pointer!important;
                background-image: none!important;
                border: 1px solid transparent!important;
                white-space: nowrap!important;
                padding: 6px 12px!important;
                font-size: 14px!important;
                line-height: 1.42857143!important;
                border-radius: 4px!important;
                -webkit-user-select: none!important;
                -moz-user-select: none!important;
                -ms-user-select: none!important;
                user-select: none!important;
                font-family: inherit!important;
                font-size: inherit!important;
                line-height: inherit!important;
                -webkit-appearance: button!important;
                cursor: pointer!important;
                text-transform: none!important;
                overflow: visible!important;
                color: inherit!important;
                font: inherit!important;
                margin: 0!important;            
            }
            .botao:hover{
                color: #fff!important;
                background-color: #286090!important;
                border-color: #204d74!important;
                text-decoration: none!important;
            }
            .customPopup{
                background-color: lightgrey;
                border-style: solid;
                border-width: 1px;
                left: 55%;
                position: fixed;
                padding: 5px;
                box-shadow: 0px 3px 7px rgba(1, 1, 1, 5);
                background: #fff;
                width: 600px;
                margin-top: 200px;
                margin-left: -200px;
                top: 10px;
                max-width: 700px;
                }
                .customPopup .close{
                position: absolute;
                top: 10px;
                right: 10px;
                transition: all 200ms;
                font-size: 20px;
                text-decoration: none;
                color: #333;
                }
                .customPopup .close:hover{
                color: red;
                }
        </style>
        
    </head>

    <body>    
        
        <div class="row">
            <div class="col-sm-12 text-center">        
                <div style="width:100%; height:10px;"></div>
                <apex:outputPanel rendered="{!panel}">

                <h2>
                    Avaliação
                </h2><br/><br/>
                      <h3>
                          <apex:form >
                          <apex:commandLink action="{!Ageral}" value="Geral"/><br/><br/>
                          <apex:commandLink action="{!Aenfermagem}" value="Enfermagem"/><br/><br/>
                          <apex:commandLink action="{!Afisioterapia}" value="Fisioterapia"/><br/><br/>
                          <apex:commandLink action="{!Afonoaudiologia}" value="Fonoaudiologia"/><br/><br/>
                          <apex:commandLink action="{!Amedica}" value="Médica"/><br/><br/>
                          <apex:commandLink action="{!Apsicologia}" value="Psicologia"/><br/><br/>
                          <apex:commandLink action="{!Aterapiaocupac}" value="Terapia Ocupacional"/><br/><br/>
                          </apex:form>
                </h3><br/>
                </apex:outputPanel>
            </div>
        </div>
        <apex:outputPanel rendered="{!valid}">
            <div class="col-sm-12 text-center"> 
            <h2 style="text-align:center">
                Cadastro de Avaliações HomeCare Hospcom
                </h2><br/><br/><br/><br/>
            </div><br/>
        <apex:form >
            <div class="row">
                <div class="col-sm-offset-2 col-sm-4 ">
                   <apex:input type="email" style="resize:none;" styleClass="form-control" html-placeholder="Insira seu e-mail..." value="{!email}" /> 
                </div>
                <div class=" col-sm-4 ">
                    <apex:inputSecret html-placeholder="Insira a senha..." styleClass="form-control" value="{!senha}"/>
                    <apex:outputText rendered="{!senhaincorreta}">
                        <p style="color:red">
                        Senha incorreta
                        </p>
                    </apex:outputText>
                    <br/>
                    <br/>                    
                </div><br/>               
            </div>
            <div class="row">
                <div class="col-sm-offset-4 col-sm-4 text-center" style="font-weight:bold;">
                    <apex:commandButton styleClass="botao" style="width:50% !important;" action="{!validar}" value="Acessar" />
                    
                </div>
            </div>
        </apex:form>
        </apex:outputPanel>
                   
        <apex:outputText rendered="{!AgeralB}">
            <div class="col-sm-12 text-center">  
                    <h2>
                        Avaliação Geral
                </h2>
            </div><br/><br/>
        <apex:form id="form" >
                <div class="form-group">
                    <br/>   
                    <div class="row">
                        <div class="col-sm-offset-2 col-sm-4 ">
                            <apex:inputText style="resize:none;" styleClass="form-control" html-placeholder="Prontuário..." value="{!prontuariog}"/>
                        </div>
                        <div class="col-sm-4 ">
                            <apex:inputText style="resize:none;" styleClass="form-control" html-placeholder="Ordem de Atendimento..." value="{!ordemAtendg}"/> 
                        </div>
                </div><br/>
                    <br/>
                    <div class="row">
                        <div class="col-sm-offset-2 col-sm-8 "> Data e hora da avaliação
                            <apex:input type="datetime-local" style="resize:none;" styleClass="form-control" value="{!dataavgeral}"/>
                        </div>
                </div><br/>
                    <div class="row">
                        <div class="col-sm-offset-2 col-sm-4 "> AIVD's
                            <apex:selectList size="1" style="resize:none;" styleClass="form-control" label="AIVDs"  value="{!AIVD}">
                                <apex:selectOption itemValue="0" itemLabel="0" />
                                <apex:selectOption itemValue="1" itemLabel="1" />
                                <apex:selectOption itemValue="2" itemLabel="2" />
                                <apex:selectOption itemValue="3" itemLabel="3" />
                                <apex:selectOption itemValue="4" itemLabel="4" />                                
                            </apex:selectList>
                        </div>
                        <div class="col-sm-4 ">AVD's
                            <apex:selectList size="1" style="resize:none;" styleClass="form-control" label="AVDs"  value="{!AVD}">
                                <apex:selectOption itemValue="0" itemLabel="0" />
                                <apex:selectOption itemValue="1" itemLabel="1" />
                                <apex:selectOption itemValue="2" itemLabel="2" />
                                <apex:selectOption itemValue="3" itemLabel="3" />
                                <apex:selectOption itemValue="4" itemLabel="4" />                                
                            </apex:selectList>                        
                        </div>
                    </div><br/>
                    <div class="row">
                        <div class="col-sm-offset-2 col-sm-4 "> Incapacidade Psíquica
                            <apex:selectList size="1" style="resize:none;" styleClass="form-control" value="{!incpsiq}">
                                <apex:selectOption itemValue="0" itemLabel="0" />
                                <apex:selectOption itemValue="1" itemLabel="1" />
                                <apex:selectOption itemValue="2" itemLabel="2" />
                                <apex:selectOption itemValue="3" itemLabel="3" />
                                <apex:selectOption itemValue="4" itemLabel="4" />                                
                            </apex:selectList>
                        </div>
                        <div class="col-sm-4 ">Vias Áereas Superiores
                            <apex:selectList size="1" style="resize:none;" styleClass="form-control" value="{!viasaereas}">
                                <apex:selectOption itemValue="0" itemLabel="0" />
                                <apex:selectOption itemValue="1" itemLabel="1" />
                                <apex:selectOption itemValue="2" itemLabel="2" />
                                <apex:selectOption itemValue="3" itemLabel="3" />
                                <apex:selectOption itemValue="4" itemLabel="4" />                                
                            </apex:selectList>                        
                        </div>
                    </div><br/>
                    <div class="row">
                        <div class="col-sm-offset-2 col-sm-4 "> Incontinência Urinária  
                            <apex:selectList size="1" style="resize:none;" styleClass="form-control" value="{!incontinencia}">
                                <apex:selectOption itemValue="0" itemLabel="0" />
                                <apex:selectOption itemValue="1" itemLabel="1" />
                                <apex:selectOption itemValue="2" itemLabel="2" />
                                <apex:selectOption itemValue="3" itemLabel="3" />
                                <apex:selectOption itemValue="4" itemLabel="4" />                                
                            </apex:selectList>
                        </div>
                        <div class="col-sm-4 ">Uso de Medicação
                            <apex:selectList size="1" style="resize:none;" styleClass="form-control" value="{!usodemedicacao}">
                                <apex:selectOption itemValue="0" itemLabel="0" />
                                <apex:selectOption itemValue="1" itemLabel="1" />
                                <apex:selectOption itemValue="2" itemLabel="2" />
                                <apex:selectOption itemValue="3" itemLabel="3" />
                                <apex:selectOption itemValue="4" itemLabel="4" />                                
                            </apex:selectList>
                        </div>
                    </div><br/>
                    <div class="row">
                        <div class="col-sm-offset-2 col-sm-4 "> Suporte Respiratório    
                            <apex:selectList size="1" style="resize:none;" styleClass="form-control" value="{!suporterespiratorio}">
                                <apex:selectOption itemValue="0" itemLabel="0" />
                                <apex:selectOption itemValue="1" itemLabel="1" />
                                <apex:selectOption itemValue="2" itemLabel="2" />
                                <apex:selectOption itemValue="3" itemLabel="3" />
                                <apex:selectOption itemValue="4" itemLabel="4" />                                
                            </apex:selectList>
                        </div>
                        <div class="col-sm-4 ">Presença de úlcera de pressão
                            <apex:selectList size="1" style="resize:none;" styleClass="form-control" value="{!presencadeulcera}">
                                <apex:selectOption itemValue="0" itemLabel="0" />
                                <apex:selectOption itemValue="1" itemLabel="1" />
                                <apex:selectOption itemValue="2" itemLabel="2" />
                                <apex:selectOption itemValue="3" itemLabel="3" />
                                <apex:selectOption itemValue="4" itemLabel="4" />                                
                            </apex:selectList>
                        </div>
                    </div><br/>
                    <div class="row">
                        <div class="col-sm-offset-2 col-sm-4 "> Reabilitação Fisioterapia e Fonoterapia     
                            <apex:selectList size="1" style="resize:none;" styleClass="form-control" value="{!reabilitacao}">
                                <apex:selectOption itemValue="0" itemLabel="0" />
                                <apex:selectOption itemValue="1" itemLabel="1" />
                                <apex:selectOption itemValue="2" itemLabel="2" />
                                <apex:selectOption itemValue="3" itemLabel="3" />
                                <apex:selectOption itemValue="4" itemLabel="4" />                                
                            </apex:selectList>
                        </div>
                        <div class="col-sm-4 ">Terapia Nutricional  
                            <apex:selectList size="1" style="resize:none;" styleClass="form-control" value="{!terapianutric}">
                                <apex:selectOption itemValue="0" itemLabel="0" />
                                <apex:selectOption itemValue="1" itemLabel="1" />
                                <apex:selectOption itemValue="2" itemLabel="2" />
                                <apex:selectOption itemValue="3" itemLabel="3" />
                                <apex:selectOption itemValue="4" itemLabel="4" />                                
                            </apex:selectList>                        
                        </div>
                    </div><br/>
                    <br/>                
                    <div class="row">
                        <div class="col-sm-offset-4 col-sm-4 text-center" style="font-weight:bold;">
                            <apex:commandLink styleClass="botao"  value="Enviar!" action="{!creategeral}"/>
                        </div>
                    </div>                  
                </div>
            </apex:form>
        
        </apex:outputText>
               
        <apex:outputText rendered="{!AenfermagemB}" >
            <div class="col-sm-12 text-center">  
                    <h2>
                        Avaliação de Enfermagem
                </h2>
            </div><br/><br/>
            <apex:form id="enf" >
                <div class="form-group">
                    <br/>   
                    <div class="row">
                        <div class="col-sm-offset-2 col-sm-4 ">
                            <apex:inputText style="resize:none;" styleClass="form-control" html-placeholder="Prontuário..." value="{!prontuarioEnf}"/>
                        </div>
                        <div class="col-sm-4 ">
                            <apex:inputText style="resize:none;" styleClass="form-control" html-placeholder="Ordem de Atendimento..." value="{!ordemAtendEnf}"/> 
                        </div>
                    </div>
                    <br/>
                    <div class="row">
                        <div class="col-sm-offset-2 col-sm-4 "> Data e Hora da Avaliação
                            <apex:input type="datetime-local" style="resize:none;" styleClass="form-control" label="AIVDs"  value="{!datavaliacaoenf}"/>
                        </div>
                    </div><br/>
                    <div class="row">
                        <div style="text-align:center; font-size:22px;">
                            Avaliação do Ambiente
                        </div><br/>
                        <div class="col-sm-offset-2 col-sm-4 "> Acesso a rede de água e esgoto
                            <apex:inputCheckbox style="resize:none;" styleClass="form-control" value="{!acessoesgoto}"/>
                        </div>
                        <div class="col-sm-4 ">Ambiente Limpo
                            <apex:inputCheckbox style="resize:none;" styleClass="form-control" value="{!ambientelimpo}"/> 
                        </div>
                    </div><br/>
                    <div class="row">
                        <div class="col-sm-offset-2 col-sm-4 "> Ambiente arejado
                            <apex:inputCheckbox style="resize:none;" styleClass="form-control"  value="{!ambientearejado}"/>
                        </div>
                        <div class="col-sm-4 ">Ambiente Organizado
                            <apex:inputCheckbox style="resize:none;" styleClass="form-control"  value="{!ambienteorganizado}"/> 
                        </div>
                    </div><br/>
                    <div class="row">
                        <div style="text-align:center; font-size:22px;">
                            Escala de Glasgow
                        </div><br/>
                        <div class="col-sm-offset-2 col-sm-8 "> Abertura ocular 
                            <apex:selectList size="1" style="resize:none;" styleClass="form-control" value="{!aberturaocular}">
                                <apex:selectOption itemValue="" itemLabel="Selecione" />                                
                                <apex:selectOption itemValue="Espontânea" itemLabel="Espontânea" />
                                <apex:selectOption itemValue="À voz" itemLabel="À voz" />
                                <apex:selectOption itemValue="À dor" itemLabel="À dor" />
                                <apex:selectOption itemValue="Nenhuma" itemLabel="Nenhuma" />
                            </apex:selectList>                        
                        </div>
                    </div><br/>
                    <div class="row">                       
                        <div class="col-sm-offset-2 col-sm-8 "> Resposta Verbal 
                            <apex:selectList size="1" style="resize:none;" styleClass="form-control" value="{!respostaverbal}">
                                <apex:selectOption itemValue="" itemLabel="Selecione" />                                
                                <apex:selectOption itemValue="Orientada" itemLabel="Orientada" />
                                <apex:selectOption itemValue="Confusa" itemLabel="Confusa" />
                                <apex:selectOption itemValue="Palavras inapropriadas" itemLabel="Palavras inapropriadas" />
                                <apex:selectOption itemValue="Palavras incompreensíveis" itemLabel="Palavras incompreensíveis" />                                
                                <apex:selectOption itemValue="Nenhuma" itemLabel="Nenhuma" />
                            </apex:selectList> 
                        </div>
                    </div><br/>
                     <div class="row">
                        <div class="col-sm-offset-2 col-sm-8 "> Resposta Motora 
                            <apex:selectList size="1" style="resize:none;" styleClass="form-control" value="{!repostamotora}">
                                <apex:selectOption itemValue="" itemLabel="Selecione" />                                
                                <apex:selectOption itemValue="Obedece a comandos" itemLabel="Obedece a comandos" />
                                <apex:selectOption itemValue="Localiza a dor" itemLabel="Localiza a dor" />
                                <apex:selectOption itemValue="Movimento de retirada" itemLabel="Movimento de retirada" />
                                <apex:selectOption itemValue="Flexão anormal" itemLabel="Flexão anormal" />
                                <apex:selectOption itemValue="Extensão anormal" itemLabel="Extensão anormal" />                                                              
                                <apex:selectOption itemValue="Nenhuma" itemLabel="Nenhuma" />
                            </apex:selectList>
                        </div>
                    </div><br/>
                    <div class="row">
                        <div style="text-align:center; font-size:22px;">
                            Escala de Ramsay
                        </div><br/>
                        <div class="col-sm-offset-2 col-sm-8 "> Sedação
                            <apex:selectList size="1" style="resize:none;" styleClass="form-control" value="{!sedacao}">
                                <apex:selectOption itemValue="" itemLabel="Selecione" />                                
                                <apex:selectOption itemValue="Acordado, agitado, colabora e atende" itemLabel="Acordado, agitado, colabora e atende" />
                                <apex:selectOption itemValue="Acordado, cooperativo, colabora e atende" itemLabel="Acordado, cooperativo, colabora e atende" />
                                <apex:selectOption itemValue="Sonolento, atende aos comandos" itemLabel="Sonolento, atende aos comandos" />
                                <apex:selectOption itemValue="Dormindo, responde rapidamente ao estímulo gabelar ou estímulo sonoro vigoroso" itemLabel="Dormindo, responde rapidamente ao estímulo gabelar ou estímulo sonoro vigoroso" />
                                <apex:selectOption itemValue="Dormindo, responde lentamente ao estímulo gabelar ou estímulo sonoro vigoroso" />                                                              
                                <apex:selectOption itemValue="Dormindo, sem resposta" itemLabel="Dormindo, sem resposta" />
                            </apex:selectList>
                        </div>
                    </div><br/>
                    <br/>
                    <div class="row">
                        <div style="text-align:center; font-size:22px;">
                            Pupilas
                        </div><br/>
                        <div class="col-sm-offset-2 col-sm-4 "> Isocóricas
                            <apex:inputText style="resize:none;" styleClass="form-control"  value="{!isocoricas}"/>
                        </div>
                        <div class="col-sm-4 ">Midríase
                            <apex:inputText style="resize:none;" styleClass="form-control"  value="{!midriase}"/> 
                        </div>
                    </div><br/>
                    <div class="row">
                        <div class="col-sm-offset-2 col-sm-4 "> Anisocóricas
                            <apex:inputText style="resize:none;" styleClass="form-control"  value="{!anisocoricas}"/>
                        </div>
                        <div class="col-sm-4 ">Reflexo fotomotor (+ ou -)
                            <apex:inputText style="resize:none;" styleClass="form-control"  value="{!reflexofotomotor}"/> 
                        </div>
                    </div><br/>
                    <div class="row">
                        <div class="col-sm-offset-2 col-sm-4 "> Miose
                            <apex:inputText style="resize:none;" styleClass="form-control"  value="{!miose}"/>
                        </div>
                    </div><br/>
                    <div class="row">
                        <div style="text-align:center; font-size:22px;">
                            Avaliação Respiratória
                        </div><br/>
                        <div class="col-sm-offset-2 col-sm-4 "> Respiração espontânea
                            <apex:inputText style="resize:none;" styleClass="form-control"  value="{!respespontanea}"/>
                        </div>
                        <div class="col-sm-4 ">Tubo Orotraqueal
                            <apex:inputText style="resize:none;" styleClass="form-control"  value="{!tuboorotraq}"/> 
                        </div>
                    </div><br/>
                    <div class="row">
                        <div class="col-sm-offset-2 col-sm-4 "> Traqueostomia
                            <apex:inputText style="resize:none;" styleClass="form-control"  value="{!traquesotomia}"/>
                        </div>
                    </div><br/>
                    <div class="row">
                        <div class="col-sm-offset-2 col-sm-4 "> Uso de O2
                            <apex:selectList size="1" style="resize:none;" styleClass="form-control" value="{!usodeO2}">
                                <apex:selectOption itemValue="" itemLabel="Selecione" />                                
                                <apex:selectOption itemValue="Cateter nasal" itemLabel="Cateter nasal" />
                                <apex:selectOption itemValue="Máscara facial" itemLabel="Máscara facial" />
                                <apex:selectOption itemValue="Por tubo em T" itemLabel="Por tubo em T" />
                                <apex:selectOption itemValue="Máscara tenda" itemLabel="Máscara tenda" />
                            </apex:selectList>                        
                        </div>
                        <div class="col-sm-4 ">Uso de O2(L/Min)
                            <apex:inputText style="resize:none;" styleClass="form-control" value="{!usodeO2lmin}"/> 
                        </div>
                    </div><br/>
                    <div class="row">
                        <div class="col-sm-offset-2 col-sm-4 "> Modo
                            <apex:inputText style="resize:none;" styleClass="form-control"  value="{!modo}"/>
                        </div>
                        <div class="col-sm-4 ">FiO2 (%) 
                            <apex:inputText style="resize:none;" styleClass="form-control"  value="{!fio2}"/> 
                        </div>
                    </div><br/>
                    <div class="row">
                        <div class="col-sm-offset-2 col-sm-4 "> Volume corrente 
                            <apex:inputText style="resize:none;" styleClass="form-control"  value="{!volumecorrente}"/>
                        </div>
                        <div class="col-sm-4 ">Pressão inspiratória     
                            <apex:inputText style="resize:none;" styleClass="form-control"  value="{!pressaoinsp}"/> 
                        </div>
                    </div><br/>
                    <div class="row">
                        <div class="col-sm-offset-2 col-sm-4 "> PEEP (cmH2O)    
                            <apex:inputText style="resize:none;" styleClass="form-control"  value="{!peep}"/>
                        </div>
                    </div><br/>
                    <div class="row">
                        <div class="col-sm-offset-2 col-sm-4 "> Pulmão direito - Murmúrios Vesiculares      
                            <apex:inputText style="resize:none;" styleClass="form-control"  value="{!pulmaodireito}"/>
                        </div>
                        <div class="col-sm-4 ">Pulmão direito - Ruídos Adventícios      
                            <apex:selectList size="1" style="resize:none;" styleClass="form-control" value="{!pulmaodireito2}">
                                <apex:selectOption itemValue="" itemLabel="Selecione" />                                
                                <apex:selectOption itemValue="Roncos" itemLabel="Roncos" />
                                <apex:selectOption itemValue="Sibilos" itemLabel="Sibilos" />
                                <apex:selectOption itemValue="Estertores" itemLabel="Estertores" />
                            </apex:selectList>
                        </div>
                    </div><br/>
                    <div class="row">
                        <div class="col-sm-offset-2 col-sm-4 "> Pulmão esquerdo - Murmúrios Vesiculares     
                            <apex:inputText style="resize:none;" styleClass="form-control"  value="{!pulmaoesquerdo}"/>
                        </div>
                        <div class="col-sm-4 ">Pulmão esquerdo - Ruídos Adventícios     
                            <apex:selectList size="1" style="resize:none;" styleClass="form-control" value="{!pulmaoesquerdo2}">
                                <apex:selectOption itemValue="" itemLabel="Selecione" />                                
                                <apex:selectOption itemValue="Roncos" itemLabel="Roncos" />
                                <apex:selectOption itemValue="Sibilos" itemLabel="Sibilos" />
                                <apex:selectOption itemValue="Estertores" itemLabel="Estertores" />
                            </apex:selectList>
                        </div>
