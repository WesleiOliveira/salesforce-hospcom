        <script>
        function dor(){
             var campoDor = document.getElementById('{!$Component.enf.dor}').value;
            
                if(campoDor == 'Relata dor ausente' || campoDor == ''){
                    document.getElementById('escaladedor').style = ('display:none;');
                    }
                else{
                    document.getElementById('escaladedor').style = ('display:block;');
                    }
            }
        function dieta(){
             var campoDieta = document.getElementById('{!$Component.enf.dieta}').value;
            
                if(campoDieta == 'Sonda'){
                    document.getElementById('sonda').style = ('display:block;');
                    }
                else{
                    document.getElementById('sonda').style = ('display:none;');
                    }
            }
        </script>
