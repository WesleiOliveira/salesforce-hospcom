    <formulas>
        <name>AvFinalizada</name>
        <dataType>Boolean</dataType>
        <expression>AND(
    OR(
 {!FormulaNDAJURIDICO},
        LEN({!obsjuridico}) &gt; 0
    ),
    OR(
        {!FormulaNDAFINAN},
        LEN({!obsFinan}) &gt; 0
    ),
    OR(
        {!FormulaNDAGOV},
        LEN({!obsGov}) &gt; 0
    )
)</expression>
    </formulas>
    <formulas>
        <name>FormulaNDAFINAN</name>
