    <formulas>
        <name>dadosCliente</name>
        <dataType>String</dataType>
        <expression>IF({!$Record.Account.CNPJ__c} != null,  &apos;&lt;b&gt;&apos; + {!$Record.Account.Raz_o_Social__c} +  &apos;&lt;/b&gt;, pessoa jurídica de direito &apos; + {!DIREITO} + &apos; inscrita no CNPJ/MF sob o nº &apos; +  {!$Record.Account.CNPJ__c} + &apos;, sediada na &apos; + {!$Record.Account.BillingStreet} + &apos;, &apos; + {!$Record.Account.BillingCity} + &apos; - &apos; + {!$Record.Account.BillingState} + &apos;, CEP: &apos; + {!$Record.Account.BillingPostalCode} + &apos;, por seu(s) representante(s) legais abaixo assinado(s), doravante denominado(a) simplesmente &lt;b&gt; CLIENTE&lt;/b&gt;.&apos;,  &apos;&lt;b&gt;&apos;  + {!$Record.Account.FirstName} + &apos; &apos; + {!$Record.Account.LastName}   + &apos;&lt;/b&gt;, pessoa física inscrita no CPF sob o n.º &apos; + {!$Record.Account.CPF__pc} + &apos;, RG: &apos; + {!$Record.Account.RG__pc} + &apos; residente à &apos; + {!$Record.Account.BillingStreet} + &apos;, &apos; + {!$Record.Account.BillingCity} + &apos; - &apos; +{!$Record.Account.BillingState} + &apos;, CEP: &apos; + {!$Record.Account.BillingPostalCode} + &apos;, abaixo assinado(s), doravante denominado(a) &lt;b&gt; CLIENTE&lt;/b&gt;&apos; )</expression>
    </formulas>
    <formulas>
        <name>demo_ou_comodato</name>
        <dataType>String</dataType>
        <expression>IF(TEXT({!$Record.Natureza_de_Opera_o__c}) = &apos;REMESSA DE COMODATO&apos;, &apos;comodato&apos;, &apos;demonstração&apos;)</expression>
    </formulas>
    <formulas>
        <name>DIREITO</name>
        <dataType>String</dataType>
        <expression>IF( TEXT({!$Record.Account.Ownership}) = &apos;PUBLICO&apos;, &apos;público&apos;, &apos;privado&apos;)</expression>
    </formulas>
