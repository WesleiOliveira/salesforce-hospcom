<?xml version="1.0" encoding="UTF-8"?>
<AutoResponseRules xmlns="http://soap.sforce.com/2006/04/metadata">
    <autoResponseRule>
        <fullName>Formulário de Orçamento do Website</fullName>
        <active>true</active>
        <ruleEntry>
            <criteriaItems>
                <field>Lead.LeadSource</field>
                <operation>equals</operation>
                <value>Web</value>
            </criteriaItems>
            <criteriaItems>
                <field>Lead.Email</field>
                <operation>notEqual</operation>
            </criteriaItems>
            <senderEmail>desenvolvimento@hospcom.net</senderEmail>
            <senderName>Comercial Hospcom</senderName>
            <template>unfiled$public/Resposta_autom_tica_a_lead_via_website</template>
        </ruleEntry>
    </autoResponseRule>
</AutoResponseRules>
