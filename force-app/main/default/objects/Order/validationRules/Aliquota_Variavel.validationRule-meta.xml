<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Aliquota_Variavel</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
    ISCHANGED(aliquota_variavel_vendedor__c),
        $Profile.Name != &quot;Administrador do sistema&quot;,
$Profile.Name != &quot;Gerente Regional&quot;   
)</errorConditionFormula>
    <errorMessage>Você não tem permissão para alterar o campo de Aliquota Variável</errorMessage>
</ValidationRule>
