<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Cotacao</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
TEXT( Pedido_da_Linha_de_OPME__c ) = &apos;NÃO&apos;,
ISNULL(QuoteId)
)</errorConditionFormula>
    <errorDisplayField>QuoteId</errorDisplayField>
    <errorMessage>Vincule o pedido a uma cotação.</errorMessage>
</ValidationRule>
