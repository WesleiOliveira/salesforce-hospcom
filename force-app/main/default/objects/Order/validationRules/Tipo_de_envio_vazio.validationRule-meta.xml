<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Tipo_de_envio_vazio</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
	!$User.Ignorar_erros__c,
        RecordType.DeveloperName == &apos;Hospcom&apos;,
        RecordType.DeveloperName == &apos;Venda_Balc_o&apos;,
	TEXT(Frete__c) == &apos;&apos;
)</errorConditionFormula>
    <errorDisplayField>Frete__c</errorDisplayField>
    <errorMessage>O tipo de envio deve ser preenchido.</errorMessage>
</ValidationRule>
