<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Finalizado_sem_participante</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
Participantes_da_demo__c == 0,
TEXT(Status) == &apos;Finalizado&apos;
)</errorConditionFormula>
    <errorMessage>A demonstração não pode ser finalizada sem a inserção dos participantes.</errorMessage>
</ValidationRule>
