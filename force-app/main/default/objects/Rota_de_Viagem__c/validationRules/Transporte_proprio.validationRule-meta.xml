<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Transporte_proprio</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
TEXT(Tipo__c) == &apos;Própria&apos;,
OR(
Motorista__c == null
)
)</errorConditionFormula>
    <errorMessage>Para transporte próprio, preencha o motorista.</errorMessage>
</ValidationRule>
