<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Conjuge_sem_fiador</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
!ISBLANK(Conjuge_do_Fiador__c),
ISBLANK(Fiador__c)
)</errorConditionFormula>
    <errorDisplayField>Fiador__c</errorDisplayField>
    <errorMessage>Não preencha cônjuge sem informar o fiador.</errorMessage>
</ValidationRule>
