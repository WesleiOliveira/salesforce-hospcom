<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Certificado_de_Calibracao_RBC</fullName>
    <active>true</active>
    <description>Validação da marcação do campo registro no conselho regional</description>
    <errorConditionFormula>AND( Certificado_de_Calibra_o__c = TRUE, ISBLANK(Data_de_Vencimento4__c))</errorConditionFormula>
