<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>tipo_de_fornecedor</fullName>
    <active>false</active>
    <errorConditionFormula>AND (ISBLANK(TEXT(TIPO__c)),  TEXT(Type) = &apos;FORNECEDOR&apos;,  TEXT(Area_da_Saude__c) == &apos;SIM&apos;)</errorConditionFormula>
    <errorDisplayField>TIPO__c</errorDisplayField>
    <errorMessage>Preencha o tipo de fornecedor</errorMessage>
</ValidationRule>
