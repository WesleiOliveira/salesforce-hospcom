<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Quantidade_devolvida_parcial</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
    ISPICKVAL(Tipo__c, &quot;Parcial&quot;),
    OR(
        ISBLANK(Quantidade_devolvida__c),
        Quantidade_devolvida__c = 0
    )
)</errorConditionFormula>
    <errorMessage>O campo Quantidade devolvida deve ser preenchido para devoluções parcial. Preencher com um valor diferente de 0.</errorMessage>
</ValidationRule>
