<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Sem_valor</fullName>
    <active>true</active>
    <errorConditionFormula>OR(
ISNULL(Produto__r.Valor_de_Venda__c),
Produto__r.Valor_de_Venda__c = 0
)</errorConditionFormula>
    <errorMessage>O produto precisa ter valor cadastrado no sistema.</errorMessage>
</ValidationRule>
