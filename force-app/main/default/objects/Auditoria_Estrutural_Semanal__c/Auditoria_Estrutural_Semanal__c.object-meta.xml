        <label>Auditoria Estrutural Semanal Name</label>
        <trackHistory>false</trackHistory>
        <type>AutoNumber</type>
    </nameField>
    <pluralLabel>Auditorias Estruturais Semanais</pluralLabel>
    <searchLayouts></searchLayouts>
