<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>VALOR_NOMINAL18_COM_UNIDADE_18</fullName>
    <active>true</active>
    <description>EXIGIR VALOR DA UNIDADE 18 SE VALOR NOMINAL DIFERENTE DE 0.</description>
