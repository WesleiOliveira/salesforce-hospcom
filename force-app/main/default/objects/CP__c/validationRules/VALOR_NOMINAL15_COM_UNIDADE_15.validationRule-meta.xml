<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>VALOR_NOMINAL15_COM_UNIDADE_15</fullName>
    <active>true</active>
    <description>EXIGIR VALOR DA UNIDADE 15 SE VALOR NOMINAL DIFERENTE DE 0.</description>
    <errorConditionFormula>AND(VN_15__c &lt;&gt; 0,  ISPICKVAL(UN_15__c, &quot;&quot;))</errorConditionFormula>
    <errorDisplayField>UN_15__c</errorDisplayField>
    <errorMessage>SELECIONE UMA UNIDADE</errorMessage>
</ValidationRule>
