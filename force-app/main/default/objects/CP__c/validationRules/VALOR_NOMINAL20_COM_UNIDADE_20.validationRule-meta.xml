<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>VALOR_NOMINAL20_COM_UNIDADE_20</fullName>
    <active>true</active>
    <description>EXIGIR VALOR DA UNIDADE 20 SE VALOR NOMINAL DIFERENTE DE 0.</description>
    <errorConditionFormula>AND(VN_20__c &lt;&gt; 0,  ISPICKVAL(UN_20__c, &quot;&quot;))</errorConditionFormula>
    <errorDisplayField>UN_20__c</errorDisplayField>
    <errorMessage>SELECIONE UMA UNIDADE</errorMessage>
</ValidationRule>
