<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>VALOR_NOMINAL19_COM_UNIDADE_19</fullName>
    <active>true</active>
    <description>EXIGIR VALOR DA UNIDADE 19 SE VALOR NOMINAL DIFERENTE DE 0.</description>
    <errorConditionFormula>AND(VN_19__c &lt;&gt; 0,  ISPICKVAL(UN_19__c, &quot;&quot;))</errorConditionFormula>
    <errorDisplayField>VN_19__c</errorDisplayField>
    <errorMessage>SELECIONE UMA UNIDADE</errorMessage>
</ValidationRule>
