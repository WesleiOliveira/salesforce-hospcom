<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>VALOR_NOMINAL16_COM_UNIDADE_16</fullName>
    <active>true</active>
    <description>EXIGIR VALOR DA UNIDADE 16 SE VALOR NOMINAL DIFERENTE DE 0.</description>
    <errorConditionFormula>AND(VN_16__c &lt;&gt; 0,  ISPICKVAL(UN_16__c, &quot;&quot;))</errorConditionFormula>
    <errorDisplayField>UN_16__c</errorDisplayField>
    <errorMessage>SELECIONE UMA UNIDADE</errorMessage>
</ValidationRule>
