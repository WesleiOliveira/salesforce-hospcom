    public static Map<String, Decimal> getLatLongFromAddress(String street, String postalCode, String city, String state) {
        String formattedAddress = street + ', ' + city + ', ' + state + ', ' + postalCode;
        String encodedAddress = EncodingUtil.urlEncode(formattedAddress, 'UTF-8');
        String apiUrl = 'https://maps.googleapis.com/maps/api/geocode/json?address=' + encodedAddress + '&key=' + API_KEY;

        HttpRequest request = new HttpRequest();
        request.setEndpoint(apiUrl);
        request.setMethod('GET');
        request.setHeader('User-Agent', 'MyApp/1.0'); // Define o User-Agent para identificar o aplicativo

        Http http = new Http();
        HttpResponse response = http.send(request);

        if (response.getStatusCode() == 200) {
            Map<String, Object> jsonResponse = (Map<String, Object>) JSON.deserializeUntyped(response.getBody());
            List<Object> results = (List<Object>) jsonResponse.get('results');
            if (!results.isEmpty()) {
                Map<String, Object> firstResult = (Map<String, Object>) results[0];
                Map<String, Object> geometry = (Map<String, Object>) firstResult.get('geometry');
                Map<String, Object> location = (Map<String, Object>) geometry.get('location');

                Decimal latitude = Decimal.valueOf((Double) location.get('lat'));
                Decimal longitude = Decimal.valueOf((Double) location.get('lng'));

                Map<String, Decimal> latLongMap = new Map<String, Decimal>();
                latLongMap.put('latitude', latitude);
                latLongMap.put('longitude', longitude);
                return latLongMap;
            } else {
                // Nenhum resultado encontrado, pode retornar uma mensagem de erro ou valor padrão
                // Exemplo: throw new CustomException('Nenhum resultado de geocodificação encontrado');
            }
        } else {
            // Tratar erros de solicitação HTTP, exemplo: throw new CustomException('Erro na solicitação HTTP');
        }

        return null; // Retornar null em caso de erro
    }


